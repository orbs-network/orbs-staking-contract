{
    "name": "orbs-staking-contract",
    "version": "1.0.0",
    "description": "Orbs Staking Smart Contracts",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/orbs-network/orbs-staking-contract.git"
    },
    "author": "Orbs",
    "license": "MIT",
    "main": "truffle-config.js",
    "files": [
        "build",
        "contracts",
        "!contracts/tests"
    ],
    "scripts": {
        "test": "truffle test",
        "coverage": "truffle run coverage",
        "profile": "WITH_GAS_PROFILER=true yarn test",
        "compile": "truffle compile",
        "lint": "yarn lint:js && yarn lint:sol",
        "lint:js": "./node_modules/.bin/eslint .",
        "lint:sol": "./node_modules/.bin/solhint --max-warnings 0 \"contracts/**/*.sol\""
    },
    "dependencies": {},
    "devDependencies": {
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.7.0",
        "babel-register": "^6.26.0",
        "bn.js": "^5.1.1",
        "chai": "^4.2.0",
        "chai-as-promised": "^7.1.1",
        "chai-bn": "^0.2.0",
        "dirty-chai": "^2.0.1",
        "eslint": "^6.8.0",
        "eslint-config-airbnb": "^18.0.1",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-react": "^7.18.3",
        "eth-gas-reporter": "^0.2.14",
        "ganache-core": "^2.9.2",
        "mocha-lcov-reporter": "^1.3.0",
        "@openzeppelin/contracts": "2.3.0",
        "@openzeppelin/test-helpers": "^0.5.4",
        "solc": "0.5.16",
        "solhint": "^2.3.0",
        "solidity-coverage": "^0.7.1",
        "truffle": "5.1.12",
        "truffle-flattener": "^1.4.2"
    }
}
